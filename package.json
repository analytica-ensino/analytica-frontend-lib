{
  "name": "analytica-frontend-lib",
  "version": "1.0.26",
  "description": "Repositório público dos componentes utilizados nas plataformas da Analytica Ensino",
  "main": "dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css",
    "./src/styles.css",
    "./dist/index.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./Text": {
      "types": "./dist/Text/Text.d.ts",
      "import": "./dist/Text/Text.mjs",
      "require": "./dist/Text/Text.js"
    },
    "./Button": {
      "types": "./dist/Button/Button.d.ts",
      "import": "./dist/Button/Button.mjs",
      "require": "./dist/Button/Button.js"
    },
    "./Badge": {
      "types": "./dist/Badge/Badge.d.ts",
      "import": "./dist/Badge/Badge.mjs",
      "require": "./dist/Badge/Badge.js"
    },
    "./Alert": {
      "types": "./dist/Alert/Alert.d.ts",
      "import": "./dist/Alert/Alert.mjs",
      "require": "./dist/Alert/Alert.js"
    },
    "./IconButton": {
      "types": "./dist/IconButton/IconButton.d.ts",
      "import": "./dist/IconButton/IconButton.mjs",
      "require": "./dist/IconButton/IconButton.js"
    },
    "./IconRoundedButton": {
      "types": "./dist/IconRoundedButton/IconRoundedButton.d.ts",
      "import": "./dist/IconRoundedButton/IconRoundedButton.mjs",
      "require": "./dist/IconRoundedButton/IconRoundedButton.js"
    },
    "./NavButton": {
      "types": "./dist/NavButton/NavButton.d.ts",
      "import": "./dist/NavButton/NavButton.mjs",
      "require": "./dist/NavButton/NavButton.js"
    },
    "./SelectionButton": {
      "types": "./dist/SelectionButton/SelectionButton.d.ts",
      "import": "./dist/SelectionButton/SelectionButton.mjs",
      "require": "./dist/SelectionButton/SelectionButton.js"
    },
    "./Table": {
      "types": "./dist/Table/Table.d.ts",
      "import": "./dist/Table/Table.mjs",
      "require": "./dist/Table/Table.js"
    },
    "./CheckBox": {
      "types": "./dist/CheckBox/CheckBox.d.ts",
      "import": "./dist/CheckBox/CheckBox.mjs",
      "require": "./dist/CheckBox/CheckBox.js"
    },
    "./TextArea": {
      "types": "./dist/TextArea/TextArea.d.ts",
      "import": "./dist/TextArea/TextArea.mjs",
      "require": "./dist/TextArea/TextArea.js"
    },
    "./DropdownMenu": {
      "types": "./dist/DropdownMenu/DropdownMenu.d.ts",
      "import": "./dist/DropdownMenu/DropdownMenu.mjs",
      "require": "./dist/DropdownMenu/DropdownMenu.js"
    },
    "./Toast": {
      "types": "./dist/Toast/Toast.d.ts",
      "import": "./dist/Toast/Toast.mjs",
      "require": "./dist/Toast/Toast.js"
    },
    "./Toast/Toaster": {
      "types": "./dist/Toast/utils/Toaster.d.ts",
      "import": "./dist/Toast/utils/Toaster.mjs",
      "require": "./dist/Toast/utils/Toaster.js"
    },
    "./Toast/ToastStore": {
      "types": "./dist/Toast/utils/ToastStore.d.ts",
      "import": "./dist/Toast/utils/ToastStore.mjs",
      "require": "./dist/Toast/utils/ToastStore.js"
    },
    "./styles.css": "./dist/index.css"
  },
  "scripts": {
    "lint": "npx eslint \"{src,app}/**/*.{js,jsx,ts,tsx}\" --fix",
    "typecheck": "npx tsc --noEmit",
    "prepare": "husky install",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:sonar": "jest --coverage --testResultsProcessor=jest-sonar-reporter",
    "generate:exports": "node scripts/generate-exports.mjs",
    "build": "npm run build:main && npm run build:components",
    "build:main": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --clean --treeshake",
    "build:components": "node scripts/build-components.mjs",
    "build:check": "npm run typecheck && npm run build",
    "dev": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --watch",
    "prepack": "npm run build:check && npm run generate:exports",
    "prepublishOnly": "npm run generate:exports",
    "check-nextjs-compat": "echo 'Checking Next.js 15 compatibility...' && npm run typecheck && npm run test",
    "ladle": "ladle serve",
    "ladle:build": "ladle build"
  },
  "engines": {
    "node": "22.x"
  },
  "packageManager": "yarn@4.9.0",
  "keywords": [
    "analytica ensino",
    "frontend",
    "components",
    "nextjs",
    "nextjs-15",
    "react-19",
    "typescript",
    "tailwindcss"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Analytica Ensino LTDA",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/analytica-ensino/analytica-frontend-lib.git"
  },
  "homepage": "https://github.com/analytica-ensino/analytica-frontend-lib#readme",
  "bugs": {
    "url": "https://github.com/analytica-ensino/analytica-frontend-lib/issues"
  },
  "peerDependencies": {
    "next": ">=15.0.0",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "dependencies": {
    "phosphor-react": "^1.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@ladle/react": "^5.0.3",
    "@tailwindcss/postcss": "^4.1.8",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsdoc": "^50.7.1",
    "eslint-plugin-prettier": "^5.4.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "jsdom": "^26.1.0",
    "next": "^15.3.3",
    "polished": "^4.3.1",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sonarqube-scanner": "^4.3.0",
    "tailwindcss": "^4.1.8",
    "ts-jest": "^29.3.4",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
